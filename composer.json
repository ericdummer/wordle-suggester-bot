{
  "name": "ericdummer/docker-php-composer",
  "description": "template repo to create a php project",
  "scripts": {
    "test": "phpunit"
  },
  "authors": [
    {
      "name": "Eric Dummer"
    }
  ],
  "require": {},
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
